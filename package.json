{
	"private": true,
	"name": "sheaft",
	"scripts": {
		"nol": "nollup -cw --port 4200 --content-base ./public",
		"rol": "rollup -cw",
		"portal:docker": "npm install && cpy ./src/configs/config.docker.js ./src/configs --rename=config.js && set NODE_ENV=LOCAL && run-p rol",
		"rollup:local": "npm install && cpy ./src/configs/config.local.js ./src/configs --rename=config.js && set NODE_ENV=LOCAL && run-p rol",
		"rollup:remote": "npm install && cpy ./src/configs/config.remote.js ./src/configs --rename=config.js && set NODE_ENV=LOCAL && run-p rol",
		"nollup:local": "npm install && cpy ./src/configs/config.local.js ./src/configs --rename=config.js && set NODE_ENV=LOCAL && run-p nol",
		"nollup:remote": "npm install && cpy ./src/configs/config.remote.js ./src/configs --rename=config.js && set NODE_ENV=LOCAL && run-p nol",
		"lint": "npx eslint --fix \"{src,!(node_modules|public|vendors|dist)/**/}*.{js,svelte}\"",
		"format": "npx prettier --write \"{src,!(node_modules|public|vendors|dist)/**/}*.{js,svelte}\"",
		"check": "svelte-check --tsconfig ./tsconfig.json --threshold error",
		"build": "set NODE_ENV=PRODUCTION && rollup -c",
		"start": "sirv public --dev --single --port 4200",
		"prepare": "husky install"
	},
